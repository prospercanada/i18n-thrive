  "name": "thrive-i18n",
  "version": "1.0.0",
  "description": "exclude footer, aria, alt,TinyMCEConten",
  "main": "index.js",
  "scripts": {
    "i18n:build:profile": "node scripts/build-from-csv.mjs i18n-build/profile-translated.csv i18n profile",
