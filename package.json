    "i18n:build:communities": "node scripts/build-from-csv.mjs i18n-build/communities-translated.csv i18n communities",
    "i18n:build:following": "node scripts/build-from-csv.mjs i18n-build/following-translated.csv i18n following",
    "i18n:build:all": "node scripts/build-all.mjs",
    "i18n:commit": "npm run i18n:build:all && git add i18n/*.json && git commit -m \"chore(i18n): rebuild translations\" || echo \"No changes\""
